<?xml version="1.0" encoding="utf-8"?>
<agent xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="urn:proactive:agent:0.90:windows">
  <config>
    <proactiveHome />
    <javaHome />
    <jvmParameters />
    <memoryLimit>0</memoryLimit>
    <nbRuntimes>1</nbRuntimes>
    <protocol>rmi</protocol>
    <portRange first="1099" last="1100" />
    <onRuntimeExitScript />
    <processPriority>Normal</processPriority>
    <maxCpuUsage>100</maxCpuUsage>
  </config>
  <events>
    <event>
      <start day="Monday" hour="8" minute="0" second="0" />
      <duration days="0" hours="11" minutes="0" seconds="0" />
      <config>
        <memoryLimit>0</memoryLimit>
        <nbRuntimes>0</nbRuntimes>
        <portRange first="0" last="0" />
        <processPriority>Normal</processPriority>
        <maxCpuUsage>100</maxCpuUsage>
      </config>
    </event>
    <event>
      <start day="Tuesday" hour="8" minute="0" second="0" />
      <duration days="0" hours="11" minutes="0" seconds="0" />
      <config>
        <memoryLimit>0</memoryLimit>
        <nbRuntimes>0</nbRuntimes>
        <portRange first="0" last="0" />
        <processPriority>Normal</processPriority>
        <maxCpuUsage>100</maxCpuUsage>
      </config>
    </event>
    <event>
      <start day="Wednesday" hour="8" minute="0" second="0" />
      <duration days="0" hours="11" minutes="0" seconds="0" />
      <config>
        <memoryLimit>0</memoryLimit>
        <nbRuntimes>0</nbRuntimes>
        <portRange first="0" last="0" />
        <processPriority>Normal</processPriority>
        <maxCpuUsage>100</maxCpuUsage>
      </config>
    </event>
    <event>
      <start day="Thursday" hour="8" minute="0" second="0" />
      <duration days="0" hours="11" minutes="0" seconds="0" />
      <config>
        <memoryLimit>0</memoryLimit>
        <nbRuntimes>0</nbRuntimes>
        <portRange first="0" last="0" />
        <processPriority>Normal</processPriority>
        <maxCpuUsage>100</maxCpuUsage>
      </config>
    </event>
    <event>
      <start day="Friday" hour="8" minute="0" second="0" />
      <duration days="0" hours="11" minutes="0" seconds="0" />
      <config>
        <memoryLimit>0</memoryLimit>
        <nbRuntimes>0</nbRuntimes>
        <portRange first="0" last="0" />
        <processPriority>Normal</processPriority>
        <maxCpuUsage>100</maxCpuUsage>
      </config>
    </event>
  </events>
  <connections>
    <localBind enabled="true">
      <respawnIncrement>10</respawnIncrement>
      <javaStarterClass>org.objectweb.proactive.core.util.winagent.PAAgentServiceRMIStarter</javaStarterClass>
    </localBind>
    <rmConnection enabled="false">
      <respawnIncrement>10</respawnIncrement>
      <javaStarterClass>org.ow2.proactive.resourcemanager.utils.RMNodeStarter</javaStarterClass>
      <url>rmi://localhost:1099</url>
    </rmConnection>
    <customConnection enabled="false">
      <respawnIncrement>10</respawnIncrement>
      <javaStarterClass>user.Starter</javaStarterClass>
    </customConnection>
  </connections>
</agent>